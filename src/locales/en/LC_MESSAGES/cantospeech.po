# about.html ----------
msgid "about_header"
msgstr "About"

msgid "about_text_1"
msgstr "I’m Amanda, a registered Speech-Language Pathologist with the College of Audiologists and Speech-Language Pathologists of Ontario (CASLPO) from Hong Kong, currently living in Canada. I provide regulated speech and language services to children and their families, with a focus on Autism Spectrum Disorder, speech sound disorders, and fluency disorders, delivered through both in-home and online sessions."

msgid "about_text_2"
msgstr "I also offer personalized online Cantonese lessons (a non-regulated service) for learners of all ages, supporting them in building confidence, enjoying the language, and progressing at their own pace."

msgid "about_professional_registrations_header"
msgstr "Professional Registrations"

msgid "about_professional_registrations_1"
msgstr "Registered Speech-Language Pathologist (College of Audiologists and Speech-Language Pathologists of Ontario)"

msgid "about_professional_registrations_2"
msgstr "Overseas Member of the Hong Kong Association of Speech Therapists"

msgid "about_qualifications_header"
msgstr "Qualifications"

msgid "about_qualifications_text_1"
msgstr "Bachelor of Arts (Major in Linguistics and Minor in Translation)"

msgid "about_qualifications_text_2"
msgstr "Master of Science in Educational Speech-Language Pathology and Learning Disabilities"

msgid "about_professional_training_header"
msgstr "Professional Training"

msgid "about_professional_trainins_text_1"
msgstr "Lidcombe Program (for Early Stuttering intervention)"

msgid "about_professional_trainins_text_2"
msgstr "Fluency Plus Program (for School-Age Stuttering intervention)"

msgid "about_professional_trainins_text_3"
msgstr "Prompts for Restructuring Oral Muscular Phonetic Targets (PROMPT) - Introduction"

msgid "about_professional_trainins_text_4"
msgstr "Oral Placement Therapy (OPT) - Level 1"


